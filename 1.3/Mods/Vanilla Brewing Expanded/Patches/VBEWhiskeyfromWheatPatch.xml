<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		  <operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName="VBE_MakeWhiskeyMashFromFlour"]/label</xpath>
				<value>
					<label>make whiskey mash from wheat</label>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName="VBE_MakeWhiskeyMashFromFlour"]/ingredients</xpath>
				<value>
					<ingredients>
						<li>
							<filter>
								<thingDefs>
									<li>DankPyon_RawWheat</li>
								</thingDefs>
							</filter>
							<count>20</count>
						</li>
					</ingredients>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName="VBE_MakeWhiskeyMashFromFlour"]/fixedIngredientFilter</xpath>
				<value>
					<fixedIngredientFilter>
						<thingDefs>
							<li>DankPyon_RawWheat</li>
						</thingDefs>
					</fixedIngredientFilter>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName="VBE_MakeWhiskeyMashFromFlour"]/defaultIngredientFilter</xpath>
				<value>
					<defaultIngredientFilter>
						<thingDefs>
							<li>DankPyon_RawWheat</li>
						</thingDefs>
					</defaultIngredientFilter>
				</value>
			</li>
			<!-- etc -->
			</operations>
	</Operation>
</Patch>
 
   