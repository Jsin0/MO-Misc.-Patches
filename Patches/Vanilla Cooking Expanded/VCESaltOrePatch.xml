<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Cooking Expanded</li>
    </mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="DankPyon_MineableSalt"]/building/mineableThing</xpath>
					<value>
						<mineableThing>VCE_Salt</mineableThing>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="DankPyon_MakeSalt"]/products</xpath>
					<value>
						<products>
							<VCE_Salt>10</VCE_Salt>
						</products>	
					</value>
				</li>
				<!--
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="DankPyon_MakeSalt10"]/products</xpath>
					<value>
						<products>
							<VCE_Salt>10</VCE_Salt>
						</products>	
					</value>
				</li>
				-->
			</operations>
		</match>	
	</Operation>
</Patch>
 
