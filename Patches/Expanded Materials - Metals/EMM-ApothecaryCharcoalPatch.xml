<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Materials - Metals</li>
			<li>[LITE]Expanded Materials - Metals</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Apothecary (Continued)</li>
			</mods>
			<match Class="PatchOperationSequence">
			  <operations>
				<li Class="PatchOperationReplace">
				  <xpath>/Defs/ProcessorFramework.ProcessDef[defName="Charcoal"]/thingDef</xpath>
				  <value>
						<thingDef>VMEu_Charcoal</thingDef>
				  </value>
				</li>
				<li Class="PatchOperationReplace">
				<xpath>Defs/RecipeDef/ingredients/li/filter/thingDefs[li="AYCharcoal"]/li</xpath>
				<value>
					<li>VMEu_Charcoal</li>
				</value>
			</li>
				</operations>
			</match>
		</match>
	</Operation>
</Patch>