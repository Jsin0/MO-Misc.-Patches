<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Plants Expanded - More Plants</li>
    </mods>
		<match Class="PatchOperationSequence">
		  <operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs</xpath>
				<value>
					<ItemProcessor.CombinationDef>
						<defName>CanolaPressed</defName>
						<building>DankPyon_Presser</building>
						<items>
							<li>VCE_RawCanola</li>
						</items>	
						<amount>
							<li>100</li>
						</amount>
						<result>Chemfuel</result>	
						<yield>25</yield>
						<useQualityIncreasing>false</useQualityIncreasing>
						<singleTimeIfNotQualityIncreasing>1</singleTimeIfNotQualityIncreasing>
					</ItemProcessor.CombinationDef>
				</value>
			</li>
			<!-- etc -->
			</operations>
		</match>	
	</Operation>
</Patch>
 
   